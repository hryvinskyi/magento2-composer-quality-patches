{
    "name": "hryvinskyi/magento2-composer-quality-patches",
    "description": "Composer plugin for automatic Magento quality patches installation",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Hryvinskyi",
            "email": "hryvinskyi@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "composer-plugin-api": "^2.0",
        "magento/quality-patches": "^1.1.75",
        "symfony/process": "^5.0|^6.0|^7.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Hryvinskyi\\MagentoComposerQualityPatches\\": "src/"
        }
    },
    "extra": {
        "class": "Hryvinskyi\\MagentoComposerQualityPatches\\Plugin"
    }
}
